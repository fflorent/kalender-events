{
  "name": "kalender-events",
  "version": "0.19.2",
  "description": "calendar events for nodejs",
  "scripts": {
    "test": "ts-mocha --paths test/**/*_spec.ts",
    "version": "standard-version --release-as minor",
    "version-bugfix": "standard-version",
    "watch": "tsc --watch",
    "build": "node ./buildSrc/esbuild.js",
    "esbuild": "node ./buildSrc/esbuild.js",
    "tsc": "tsc",
    "tsc-declaration": "tsc --declaration --emitDeclarationOnly",
    "compile": "npm run esbuild && npm run tsc-declaration && npm run tsc",
    "prepack": "npm run compile",
    "release": "release-it"
  },
  "bin": {
    "kalender-events": "./bin/kalender-events.js",
    "kev": "./bin/kalender-events.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/naimo84/kalender-events.git"
  },
  "keywords": [
    "ics",
    "events",
    "ical",
    "google",
    "calendar",
    "caldav",
    "icloud"
  ],
  "exports": "./dist/index.js",
  "types": "dist/index.d.ts",
  "author": "naimo84",
  "dependencies": {
    "@naimo84/dav": "^1.9.0",
    "axios": "^0.21.1",
    "cloneextend": "0.0.3",
    "commander": "^8.3.0",
    "crypto-js": "^3.1.9-1",
    "debug": "^4.1.1",
    "globby": "^11.0.4",
    "ical-expander": "^2.1.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "node-cache": "^5.1.0",
    "node-ical": "0.10.0",
    "rrule": "2.6.4",
    "scrapegoat": "1.0.1",
    "uuidv4": "^6.2.12",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/expect": "^24.3.0",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^8.2.0",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^13.11.0",
    "chai": "4.2.0",
    "chai-like": "1.1.1",
    "chai-things": "0.2.0",
    "commitlint": "^8.3.5",
    "esbuild": "^0.13.15",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^2.12.2",
    "gulp-watch": "^4.0.1",
    "mocha": "^9.1.3",
    "release-it": "^14.11.8",
    "sinon": "9.0.1",
    "standard-version": "^9.3.2",
    "ts-mocha": "^8.0.0",
    "typescript": "^3.8.3"
  },
  "prepublish": "tsc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/naimo84/kalender-events/issues"
  },
  "homepage": "https://github.com/naimo84/kalender-events#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit"
    }
  },
  "funding": {
    "type": "paypal",
    "url": "https://paypal.me/NeumannBenjamin"
  },
  "standard-version": {
    "skip": {
      "bump": true,
      "commit": true,
      "tag": true
    }
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    }
  },
  "commitlint": {
    "format": {
      "helpUrl": "test"
    },
    "rules": {
      "body-leading-blank": [
        1,
        "always"
      ],
      "footer-leading-blank": [
        1,
        "always"
      ],
      "header-max-length": [
        2,
        "always",
        72
      ],
      "scope-case": [
        2,
        "always",
        "lower-case"
      ],
      "subject-case": [
        2,
        "never",
        [
          "sentence-case",
          "start-case",
          "pascal-case",
          "upper-case"
        ]
      ],
      "subject-empty": [
        2,
        "never"
      ],
      "subject-full-stop": [
        2,
        "never",
        "."
      ],
      "type-case": [
        2,
        "always",
        "lower-case"
      ],
      "type-empty": [
        2,
        "never"
      ],
      "type-enum": [
        2,
        "always",
        [
          "build",
          "chore",
          "ci",
          "docs",
          "feat",
          "fix",
          "perf",
          "refactor",
          "revert",
          "style",
          "test"
        ]
      ]
    }
  }
}
