{
  "name": "kalender-events",
  "version": "0.1.3",
  "description": "calendar events for nodejs",
  "main": "dist/lib.js",
  "scripts": {
    "start": "node-red",
    "test": "mocha \"test/**/*_spec.js\"",
    "release": "standard-version",
    "release-minor": "standard-version --release-as minor",
    "watch": "tsc -w -p ./tsconfig.json",
    "dev": "gulp develop",
    "build": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/naimo84/kalender-events.git"
  },
  "keywords": [
    "ics",
    "events",
    "ical",
    "google",
    "calendar",
    "caldav",
    "icloud"
  ],
  "types": "types/index.d.ts",
  "author": "naimo84",
  "dependencies": {
    "axios": "^0.19.2",
    "cloneextend": "0.0.3",
    "crypto-js": "^3.1.9-1",
    "dav": "^1.8.0",
    "debug": "^4.1.1",
    "ical-expander": "^2.1.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "node-cache": "^5.1.0",
    "node-ical": "0.10.0",
    "rrule": "2.6.4",
    "scrapegoat": "^1.0.1",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/dav": "^1.7.1",
    "@types/lodash": "^4.14.149",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^13.11.0",
    "commitlint": "^8.3.5",
    "gulp": "^4.0.2",
    "gulp-nodemon": "^2.4.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-watch": "^5.0.1",
    "standard-version": "^7.1.0",
    "typescript": "^3.8.3",
    "sinon": "9.0.1",
    "chai": "4.2.0",
    "chai-like": "1.1.1",
    "chai-things": "0.2.0",
    "mocha": "7.1.1"
  },
  "prepublish": "tsc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/naimo84/kalender-events/issues"
  },
  "homepage": "https://github.com/naimo84/kalender-events#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit"
    }
  },
  "funding": {
    "type": "paypal",
    "url": "https://paypal.me/NeumannBenjamin"
  },
  "commitlint": {
    "format": {
      "helpUrl": "test"
    },
    "rules": {
      "body-leading-blank": [
        1,
        "always"
      ],
      "footer-leading-blank": [
        1,
        "always"
      ],
      "header-max-length": [
        2,
        "always",
        72
      ],
      "scope-case": [
        2,
        "always",
        "lower-case"
      ],
      "subject-case": [
        2,
        "never",
        [
          "sentence-case",
          "start-case",
          "pascal-case",
          "upper-case"
        ]
      ],
      "subject-empty": [
        2,
        "never"
      ],
      "subject-full-stop": [
        2,
        "never",
        "."
      ],
      "type-case": [
        2,
        "always",
        "lower-case"
      ],
      "type-empty": [
        2,
        "never"
      ],
      "type-enum": [
        2,
        "always",
        [
          "build",
          "chore",
          "ci",
          "docs",
          "feat",
          "fix",
          "perf",
          "refactor",
          "revert",
          "style",
          "test"
        ]
      ]
    }
  }
}
